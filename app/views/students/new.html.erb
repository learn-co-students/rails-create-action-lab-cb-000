<h3>Student Form </h3>
<%= form_tag students_path   do %>
  <label for = "first_name">First name:</label>
  <%= text_field_tag :'first_name', nil, id: 'first_name' %><br><br>
  <label for = "first_name">Last name:</label>
  <%= text_field_tag :'last_name', nil, id: 'last_name' %><br><br>
  <!--<%= hidden_field_tag :'authenticity_token', form_authenticity_token %> -->
  <%= submit_tag :"Submit Student", id: "Submit Student" %>
<% end %>
<%= params.inspect %>
